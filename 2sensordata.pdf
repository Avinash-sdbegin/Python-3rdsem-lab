import csv

def write_and_read_sensor_data():
    filename = "sensor_data.csv"

    # Sample sensor readings
    data = [
        ["Sensor", "Temperature(Â°C)", "Humidity(%)"],
        ["Sensor_A", 24.5, 55],
        ["Sensor_B", 27.2, 60],
        ["Sensor_C", 23.9, 58],
    ]

    # Write data to CSV file
    with open(filename, mode="w", newline="") as file:
        writer = csv.writer(file)
        writer.writerows(data)
    print("Sensor data written to CSV successfully!\n")

    # Read back the CSV data
    print("Reading data back from CSV:\n")
    with open(filename, mode="r") as file:
        reader = csv.reader(file)
        for row in reader:
            print(row)

write_and_read_sensor_data()
